{
  "name": "ash.coffee",
  "version": "0.0.7",
  "description": "Ash port to Coffeescript",
  "keywords": [
    "ash",
    "coffee-script"
  ],
  "main": "index.js",
  "scripts": {
    "build": "cake -c ADVANCED_OPTIMIZATIONS make",
    "clean": "rimraf build/*",
    "dist": "cake dist",
    "get": "bower-installer",
    "postinstall": "bower install && npm run get",
    "prebuild": "npm run clean -s && npm run transpile && npm run resources",
    "publish": "cake publish",
    "resources": "cp -fr lib/res web/res",
    "start": "node ./tools/server.js",
    "serve": "node ./tools/server.js build/web",
    "transpile": "coffee -o web/packages/ash -cm lib",
    "test": "NODE_ENV=test mocha --reporter nyan"
  },
  "author": "darkoverlordofdata",
  "contributors": [
    {
      "name": "bruce davidson",
      "email": "brucedavidson@darkoverlordofdata.com"
    }
  ],
  "directories": {
    "lib": "./lib",
    "example": "./example"
  },
  "repository": "git://github.com/darkoverlordofdata/ash.coffee",
  "license": "MIT",
  "devDependencies": {
    "browserify": "~6.2.0",
    "chai": "~1.9.2",
    "coffee-script": "~1.9.3",
    "executive": "^0.5.0",
    "fs-extra": "^0.22.1",
    "gulp": "^3.9.0",
    "gulp-gh-pages": "^0.5.2",
    "gulp-manifest": "0.0.7",
    "gulp-shell": "^0.4.2",
    "gulp-webserver": "^0.9.1",
    "liquid.coffee": "^0.1.1",
    "mocha": "~2.0.1",
    "morgan": "^1.6.1",
    "rimraf": "^2.4.2"
  },
  "dependencies": {}
}
