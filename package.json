{
  "name": "ash.coffee",
  "version": "0.0.8",
  "description": "Ash port to Coffeescript",
  "keywords": [
    "ash",
    "coffee-script"
  ],
  "main": "index.js",
  "scripts": {
    "build": "cake build",
    "clean": "rimraf build/*",
    "get": "bower-installer",
    "gh-pages": "cake publish",
    "postget": "ncp packages/closure-compiler/lib/vendor/compiler.jar ./tools",
    "postbuild": "ncp build/ash.js build/web/src/ash/ash.js",
    "postinstall": "bower install && npm run get",
    "prebuild": "npm run clean -s && npm run transpile",
    "start": "node ./tools/server.js",
    "serve": "node ./tools/server.js build/web",
    "transpile": "coffee -o web/src/ash -cm lib && coffee -o web/src/example -cm example/src",
    "test": "NODE_ENV=test mocha --compilers coffee:coffee-script --require test/test_helper.js --recursive"
  },
  "config": {},
  "author": "darkoverlordofdata",
  "contributors": [
    {
      "name": "bruce davidson",
      "email": "brucedavidson@darkoverlordofdata.com"
    }
  ],
  "directories": {
    "lib": "./lib",
    "example": "./example"
  },
  "repository": "git://github.com/darkoverlordofdata/ash.coffee",
  "license": "MIT",
  "devDependencies": {
    "browserify": "~6.2.0",
    "chai": "~1.9.2",
    "coffee-script": "~1.9.3",
    "executive": "^0.5.0",
    "fs-extra": "^0.22.1",
    "gulp": "^3.9.0",
    "gulp-gh-pages": "^0.5.2",
    "gulp-manifest": "0.0.7",
    "gulp-shell": "^0.4.2",
    "gulp-webserver": "^0.9.1",
    "liquid.coffee": "^0.1.1",
    "mocha": "~2.0.1",
    "morgan": "^1.6.1",
    "ncp": "^2.0.0",
    "rimraf": "^2.4.2",
    "shelljs": "^0.5.1"
  },
  "dependencies": {}
}
