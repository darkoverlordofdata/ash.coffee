{
  "name": "ash",
  "version": "0.1.0",
  "authors": [
    "darkoverlordofdata <darkoverlordofdata@gmail.com>"
  ],
  "description": "Ash Framework port to coffeescript",
  "license": "MIT",
  "private": true,
  "main": "build/ash.js",
  "ignore": [
    "**/.*",
    "build/lib",
    "build/web",
    "node_modules",
    "packages",
    "test",
    "web/src"
  ],
  "dependencies": {
    "box2d.js": "git://github.com/flyover/box2d.js",
    "box2dweb": "git://github.com/darkoverlordofdata/Box2dWeb",
    "stats.js": "*"
  },
  "devDependencies": {
    "closure-compiler": "~0.2.7",
    "google-closure-library": "*"
  },
  "install": {
    "ignore": [
      "box2d.js",
      "closure-compiler",
      "google-closure-library"
    ],
    "path": "web/src",
    "sources": {
      "box2dweb": "packages/box2dweb/dist/box2d.js",
      "stats.js": "packages/stats.js/build/stats.min.js"
    }
  }
}
