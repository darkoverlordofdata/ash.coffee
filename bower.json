{
  "name": "ash",
  "version": "0.1.0",
  "authors": [
    "darkoverlordofdata <darkoverlordofdata@gmail.com>"
  ],
  "description": "Ash Framework port to coffeescript",
  "license": "MIT",
  "private": true,
  "main": "build/ash.js",
  "ignore": [
    "**/.*",
    "bin",
    "tmp",
    "build/web",
    "node_modules",
    "packages",
    "web/packages",
    "tmp",
    "test",
    "tests"
  ],
  "dependencies": {
    "stats.js": "*",
    "cocoonjs": "git://github.com/ludei/cocoonjs-plugins#~3.0.4",
    "box2d": "git://github.com/darkoverlordofdata/Box2dWeb"
  },
  "devDependencies": {
    "closure-compiler": "~0.2.7"
  },
  "install": {
    "path": "web/src",
    "sources": {
      "closure-compiler": "packages/closure-compiler/lib/vendor/compiler.jar",
      "stats.js": "packages/stats.js/build/stats.min.js",
      "cocoonjs": "packages/cocoonjs/build/cocoon.min.js",
      "box2d": "packages/box2d/dist/box2d.min.js"
    }
  }
}
